{
  "name": "Glassdoor-Linkedinator",
  "description": "Shows Glassdoor reviews for companies when searching on Linkedin",
  "version": "1.0",

  "content_scripts": 
    [
        {
            "matches":["https://www.linkedin.com/vsearch/*"],
            "js":["jquery-2.1.3.min.js","var.js", "linker.js"],
            "run_at": "document_end"
        }
    ],
  "permissions": [
    "https://api.glassdoor.com/*","storage"
  ],
  "manifest_version": 2
}